{% extends 'admin/include/main.html.twig' %}

{#{% titre de l'activite %}#}
{% block title_job %}
	<h1 class="card-title">
		{{  'Nouveau Parametrage' }}
	</h1>
{% endblock %}

{# {% operation possible %}#}
{% block option %}


	<a href="{{ path('app_parametrage_app_index') }}" class="m-1 btn btn-info float-right btn-sm">
		<i class="fa fa-list" aria-hidden="true"></i>Liste
	</a>

	<a href="{{ path('app_parametrage_app_edit', {'id': parametrage.id}) }}" class="m-1 btn btn-info float-right btn-sm">
		<i class="fas fa-edit    "></i>
	</a>


{% endblock %}

{# {% les donnees affichees %}#}
{% block data %}

	<div class="row">
		<div class="col-12">
			<div class="card card-margin">
				<div class="card-body">
					<div class="row search-body">
						<div class="col-lg-12">
							<div class="search-result">

								<div class="result-body">
									<div class="table-responsive">

										<table class="table">
											<tbody>
												<tr>
													<th>Id</th>
													<td>{{ parametrage.id }}</td>
												</tr>
												<tr>
													<th>Logo</th>
													<td>{{ parametrage.logo }}</td>
												</tr>
												<tr>
													<th>Name</th>
													<td>{{ parametrage.name }}</td>
												</tr>
												<tr>
													<th>Description</th>
													<td>{{ parametrage.description }}</td>
												</tr>
											</tbody>
										</table>


									</div>
								</div>
							</div>
						</div>
					</div>

				</div>
			</div>
		</div>
	</div>
{% endblock %}
